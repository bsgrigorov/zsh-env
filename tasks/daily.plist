<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>Label</key>
    <string>com.cron.daily</string>
    <key>ProgramArguments</key>
    <array>
        <string>sh</string>
        <string>-c</string>
        <string>~/Development/Repos/home/zsh-env/tasks/daily.sh</string>
    </array>
    <key>StandardOutPath</key>
    <string>~/Development/Repos/home/zsh-env/tasks/daily.log</string>
    <key>StandardErrorPath</key>
    <string>~/Development/Repos/home/zsh-env/tasks/daily.log</string>
    <key>StartCalendarInterval</key>
    <dict>
        <key>Hour</key>
        <integer>0</integer>
        <key>Minute</key>
        <integer>6</integer>
    </dict>
</dict>
</plist>